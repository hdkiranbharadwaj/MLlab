   "source": [
    "# 3D Plot\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "dataset = pd.read_csv('./ToyotaCorolla.csv')\n",
    "x = dataset['KM']\n",
    "y = dataset['Doors']\n",
    "z = dataset['Price']\n",
    "\n",
    "ax = plt.axes(projection='3d')\n",
    "ax.plot_trisurf(x,y,z,cmap=\"jet\")\n",
    "ax.set_title(\"3D Surface Plot\")\n",
    "\n",
    "plt.show()"
   ]
  }
